{
  "name": "generative-doom",
  "version": "1.0.0",
  "description": "generating rhymes based on DOOM's corpus",
  "main": "src/js/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist && cp static/* dist/ && mkdir dist/data && cp -R data/* dist/data/",
    "build": "webpack --optimize-minimize --optimize-dedupe",
    "start": "webpack-dev-server --content-base dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "rhymes",
    "DOOM",
    "markov",
    "natural",
    "language",
    "processing",
    "generative",
    "literature",
    "poetry"
  ],
  "author": "nabilhassein",
  "license": "WTFPL",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "gh-pages": "^0.11.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "pronouncing": "github:aparrish/pronouncingjs"
  }
}
